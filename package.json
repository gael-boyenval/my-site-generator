{
  "name": "my-site-generator",
  "version": "1.0.0",
  "author": "Gael BOYENVAL",
  "description": "my static site generator using gael-boyenval.github.io as subtree",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gael-boyenval/my-site-generator.git"
  },
  "scripts": {
    "test": "jest",
    "lint": "eslint 'src/**/*.js'",
    "develop": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "publish": "git subtree push --prefix public sitedeploy master",
    "storybook": "start-storybook -p 9001 -c .storybook"
  },
  "dependencies": {
    "gatsby": "latest",
    "gatsby-link": "latest",
    "gatsby-plugin-styled-components": "latest",
    "gatsby-source-filesystem": "latest",
    "gatsby-source-medium": "^1.0.14",
    "gatsby-transformer-remark": "latest",
    "styled-components": "latest"
  },
  "devDependencies": {
    "babel-eslint": "latest",
    "babel-jest": "^22.4.3",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-15": "^1.0.5",
    "eslint": "latest",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-react": "^7.7.0",
    "husky": "^0.15.0-rc.13",
    "jest": "^22.4.3",
    "jest-styled-components": "^5.0.0",
    "plop": "^2.0.0",
    "react-test-renderer": "^15.5.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint",
      "pre-push": "CI=true yarn test"
    }
  }
}
